<!DOCTYPE html>
<html lang="en">
<head>
  <title></title>
</head>
<body>
    <div class="table-data">
<h2>Product Listing</h2>
    <table border="1">
        <tr>
            <th>id</th>
            <th>Product Name</th>
            <th>Price</th>
            <th>Image URL</th>
            <th>Delete</th>
        </tr>
        
        <%
        if(productData.length!=0){
        var i=1;
        productData.forEach(function(data){
        %>
        <tr>
            <td><%=i; %></td>
            <td><%=data.title %></td>
            <td><%=data.price %></td>
            <td><%=data.image %></td>
            <td><a href="/products/delete/<%=data.id%>">Delete</a></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
    </div>
</body>
</html>